<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_ICourses" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:6789/" binding="netTcpBinding"
